<?php

/**
 * @file
 * Contains promotion_page.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function promotion_page_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the promotion_page module.
    case 'help.page.promotion_page':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('RHDP promotion_page Content Type') . '</p>';
      return $output;

    default:
  }
}
