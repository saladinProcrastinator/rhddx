{
  "name": "rhddx",
  "version": "2.0.0-prerelease.9",
  "description": "Front-end assets and documentation for Red Hat Developer site",
  "main": "index.js",
  "engines": {
    "node": ">=10.10.0"
  },
  "scripts": {
    "build": "gulp build",
    "jekyll": "bundle exec jekyll build",
    "install": "bundle install",
    "submodule": "git submodule init && cd developers.redhat.com/ && git submodule update && cd _docker/drupal/drupal-filesystem/web/themes/custom/rhdp2/rhd-frontend/ && npm install && cd ../../../../../../../../../",
    "deployment": "gulp deploy",
    "development": "gulp development",
    "changelog": "gren release -P --override && gren changelog --override",
    "push-tags": "git push origin master --follow-tags",
    "release": "npm run push-tags && npm run changelog"
  },
  "sass": {
    "includePaths": [
      "./node_modules"
    ]
  },
  "repository": "https://github.com/mindreeper2420/rhddx.git",
  "url": "https://rhddx.netlify.com",
  "author": "Adam J. Jolicoeur",
  "license": "MIT",
  "devDependencies": {
    "@patternfly/patternfly": "2.56.3",
    "@skpm/child_process": "0.4.1",
    "autoprefixer": "9.7.3",
    "browser-sync": "2.26.7",
    "cssnano": "4.1.10",
    "del": "5.1.0",
    "flat-map": "1.0.0",
    "github-release-notes": "0.17.1",
    "gulp": "4.0.2",
    "gulp-clean": "0.4.0",
    "gulp-exec": "4.0.0",
    "gulp-header": "2.0.9",
    "gulp-imagemin": "6.2.0",
    "gulp-newer": "1.4.0",
    "gulp-noop": "1.0.0",
    "gulp-postcss": "8.0.0",
    "gulp-sass": "4.0.2",
    "gulp-scale-images": "2.0.1",
    "gulp-size": "3.0.0",
    "gulp-sourcemaps": "2.6.5",
    "imagemin-mozjpeg": "8.0.0",
    "imagemin-pngquant": "8.0.0"
  }
}
